<Window x:Class="WpfAppGridspelletje.VictoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppGridspelletje"
        mc:Ignorable="d"
        Title="VictoryWindow" Height="200" Width="260">
    <Grid>
        <Image Source="img/Gefeliciteerd.jpg"/>
        <TextBlock x:Name="tbScore" VerticalAlignment="Bottom" Height="15" Margin="0,0,0,10" 
                   TextAlignment="Center" FontFamily="Comic Sans MS" FontWeight="Bold" FontSize="10"/>
    </Grid>
</Window>
